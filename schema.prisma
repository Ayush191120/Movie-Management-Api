generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id          String      @id @default(uuid())
  title       String
  director    String?
  releaseYear Int
  genre       String?
  rating      Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}



